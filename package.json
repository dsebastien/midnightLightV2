{
  "name": "midnight-light",
  "description": "Midnight Light is a modern personal Web front-end.",
  "author": {
	"email": "seb@dsebastien.net",
	"name": "Sebastien Dubois",
	"url": "https://www.dsebastien.net/"
  },
  "contributors": [],
  "homepage": "https://www.dsebastien.net",
  "keywords": [
	"angular2",
	"angularjs",
	"es6",
	"frontend",
	"html5",
	"typescript",
	"web",
	"wordpress"
  ],
  "version": "1.1.0",
  "jspm": {
	"configFile": "jspm.conf.js",
	"dependencies": {
	  "@reactivex/rxjs": "npm:@reactivex/rxjs@5.0.0-alpha.7",
	  "angular2": "npm:angular2@^2.0.0-alpha.46",
	  "babel-runtime": "npm:babel-runtime@^6.1.18",
	  "font-awesome": "npm:font-awesome@^4.4.0",
	  "lodash": "npm:lodash@^3.10.1",
	  "normalize.css": "github:necolas/normalize.css@^3.0.3",
	  "reflect-metadata": "npm:reflect-metadata@^0.1.2",
	  "zone.js": "npm:zone.js@^0.5.8"
	},
	"devDependencies": {
	  "babel": "npm:babel-core@^6.1.21",
	  "core-js": "npm:core-js@^1.2.1"
	}
  },
  "dependencies": {
	"babel-runtime": "6.1.x",
	"angular2": "2.0.0-alpha.46",
	"@reactivex/rxjs": "5.0.0-alpha.7",
	"reflect-metadata": "0.1.2",
	"zone.js": "0.5.8"
  },
  "devDependencies": {
	"babel-core": "6.3.x",
	"babel-plugin-transform-es2015-modules-commonjs": "6.3.x",
	"babel-preset-es2015": "6.3.x",
	"gulp": "3.9.x",
	"jspm": "0.16.x",
	"nodemon": "1.8.x",
	"typescript": "1.8.0-dev.20151213",
	"modern-web-dev-build": "0.1.1",
	"semantic-release": "4.3.x",
	"tsd": "0.6.5"
  },
  "engines": {
	"node": ">=4.2.1",
	"npm": ">=2.0.0"
  },
  "private": true,
  "license": "MIT",
  "bugs": "https://github.com/dsebastien/midnightLightV2/issues",
  "repository": {
	"type": "git",
	"url": "git@github.com:dsebastien/midnightLightV2.git"
  },
  "scripts": {
	"semantic-release": "./node_modules/.bin/semantic-release pre && npm publish && semantic-release post",
	"clean": "./node_modules/.bin/gulp clean",
	"build": "./node_modules/.bin/gulp",
	"gulp": "npm run build",
	"test": "./node_modules/.bin/gulp test-unit",
	"start": "npm run serve",
	"serve": "./node_modules/.bin/nodemon --watch gulpfile.js --watch gulpfile.babel.js --watch package.json --watch .jshintrc --watch .jscsrc --watch tsconfig.json --watch tslint.json --watch jspm.conf.js --exec ./node_modules/.bin/gulp serve",
	"serve-dist": "./node_modules/.bin/nodemon --watch gulpfile.js --watch gulpfile.babel.js --watch package.json --watch .jshintrc --watch .jscsrc --watch tsconfig.json --watch tslint.json --watch jspm.conf.js --exec ./node_modules/.bin/gulp serve-dist",
	"update": "npm install --no-optional && ./node_modules/.bin/jspm update && ./node_modules/.bin/jspm dl-loader && npm run tsd",
	"outdated": "npm outdated",
	"help": "./node_modules/.bin/gulp help",
	"tsd": "./node_modules/.bin/tsd reinstall --save --overwrite",
	"tsd-update": "./node_modules/.bin/tsd update --save --overwrite",
	"setup": "npm install --no-optional && jspm install && jspm dl-loader && npm run tsd"
  }
}
